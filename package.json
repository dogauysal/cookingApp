{
  "name": "cookingapp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android && npm run db",
    "ios": "react-native run-ios && npm run db",
    "start": "react-native start && npm run db",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "db": "npx json-server --watch db.json",
    "reset": "react-native start --reset-cache"
  },
  "dependencies": {
    "@babel/preset-env": "^7.18.10",
    "@babel/preset-flow": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@react-native-async-storage/async-storage": "^1.17.10",
    "@react-native-material/core": "^1.3.7",
    "@react-navigation/native": "^6.0.11",
    "@react-navigation/native-stack": "^6.7.0",
    "axios": "^0.27.2",
    "jest-config": "^28.1.3",
    "json-server": "^0.17.0",
    "react": "18.0.0",
    "react-native": "0.69.4",
    "react-native-numeric-input": "^1.9.1",
    "react-native-safe-area-context": "^4.3.1",
    "react-native-screens": "^3.16.0",
    "react-native-svg": "^12.4.0",
    "react-native-vector-icons": "^9.2.0",
    "ts-jest": "^28.0.8"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "@tsconfig/react-native": "^2.0.2",
    "@types/jest": "^26.0.23",
    "@types/node": "^18.7.6",
    "@types/react-native": "^0.69.3",
    "@types/react-native-vector-icons": "^6.4.11",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "babel-jest": "^26.6.3",
    "eslint": "^7.32.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "^0.70.3",
    "react-test-renderer": "18.0.0",
    "typescript": "^4.4.4"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(js)$": "<rootDir>/node_modules/babel-jest",
      "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "testPathIgnorePatterns": [
      "\\.snap$",
      "<rootDir>/node_modules/"
    ],
    "cacheDirectory": ".jest/cache"
  }
}
